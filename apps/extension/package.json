{
	"name": "z3us-extension",
	"version": "1.0.11",
	"scripts": {
		"dev": "yarn start",
		"start": "vite",
		"build": "yarn build:chrome && yarn build:firefox",
		"build:chrome": "export APP_TARGET=chrome && npm_config_build_from_source=true tsc && vite build",
		"build:firefox": "export APP_TARGET=firefox && npm_config_build_from_source=true tsc && vite build",
		"build:dev": "yarn build:chrome-dev && yarn build:firefox-dev",
		"build:chrome-dev": "yarn build:chrome --mode development",
		"build:firefox-dev": "yarn build:firefox --mode development",
		"preview": "vite preview",
		"check-types": "tsc --noemit",
		"eslint": "eslint --ignore-path .eslintignore --ext .js,.ts,.tsx .",
		"lint": "yarn run eslint && yarn run check-types",
		"test": "jest --maxWorkers=1  --detectOpenHandles --forceExit"
	},
