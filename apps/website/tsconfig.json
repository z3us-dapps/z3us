{
	"extends": "tsconfig/nextjs.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/config/": ["config/index.ts"],
			"@/public/*": ["public/*"],
			"@/components/*": ["components/*"],
			"@/hooks/*": ["hooks/*"],
			"@/docs/*": ["docs/*"],
			"@/utils/*": ["utils/*"]
		}
	},
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
	"exclude": ["node_modules"]
}
