		"async-mutex": "^0.4.0",
		"bip39": "^3.1.0",
		"blakejs": "^1.2.1",
		"buffer": "^6.0.3",
		"clsx": "^1.2.1",
		"crypto-browserify": "^3.12.0",
		"elliptic": "^6.5.4",
		"immer": "^10.0.2",
		"neverthrow": "^6.0.0",
		"process": "^0.11.10",
		"qrcode.react": "^3.1.0",
