{
	"name": "ui",
	"version": "0.0.0",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"sideEffects": false,
	"private": true,
	"license": "MIT",
	"files": [
		"dist/**"
	],
	"scripts": {
		"storybook": "start-storybook --no-open -s public -p 6006",
		"storybook:build": "build-storybook -s public",
		"test": "jest --clearCache --detectOpenHandles --forceExit --coverage",
		"build": "tsup \"src/**/*.{ts,tsx}\" --format esm,cjs --dts --external react",
		"dev": "tsup \"src/**/*.{ts,tsx}\" --format esm,cjs --watch --dts --external react",
		"check-types": "tsc --noemit",
		"eslint": "TIMING=1 eslint src --fix",
		"lint": "npm run eslint && npm run check-types",
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
	},
	"devDependencies": {
		"@storybook/addon-a11y": "^6.4.22",
		"@storybook/addon-actions": "^6.4.22",
		"@storybook/addon-essentials": "^6.4.22",
		"@storybook/addon-links": "^6.4.22",
		"@storybook/addon-storysource": "^6.4.22",
		"@storybook/addon-viewport": "^6.4.22",
		"@storybook/cli": "^6.4.22",
		"@storybook/preset-scss": "^1.0.3",
		"autoprefixer": "^10.4.0",
		"css-loader": "5.2.6",
		"msw": "^0.36.7",
		"msw-storybook-addon": "^1.6.3",
		"sass": "^1.49.9",
		"sass-loader": "10.1.1",
		"storybook-addon-designs": "^6.2.1",
		"storybook-addon-pseudo-states": "^1.9.0",
		"storybook-dark-mode": "^1.1.0",
		"style-loader": "2.0.0",
		"swr": "^1.2.0",
		"tsconfig": "*",
		"tsup": "^5.11.13",
		"typescript": "^4.6.4"
	},
	"msw": {
		"workerDirectory": "public"
	},
	"dependencies": {
		"@radix-ui/popper": "^0.1.0",
		"@radix-ui/react-accordion": "^0.1.6",
		"@radix-ui/react-alert-dialog": "^0.1.7",
		"@radix-ui/react-avatar": "^0.1.4",
		"@radix-ui/react-checkbox": "^0.1.5",
		"@radix-ui/react-dialog": "^0.1.7",
		"@radix-ui/react-dropdown-menu": "^0.1.6",
		"@radix-ui/react-hover-card": "^0.1.5",
		"@radix-ui/react-icons": "^1.0.3",
		"@radix-ui/react-polymorphic": "^0.0.14",
		"@radix-ui/react-popover": "^0.1.6",
		"@radix-ui/react-select": "^0.1.1",
		"@radix-ui/react-slider": "^0.1.4",
		"@radix-ui/react-tabs": "^0.1.5",
		"@radix-ui/react-toggle": "^0.1.4",
		"@radix-ui/react-tooltip": "^0.1.7",
		"@stitches/react": "^1.2.8",
		"@storybook/react": "^6.4.15",
		"@types/react": "^17.0.39",
		"@types/react-dom": "^17.0.11",
		"framer-motion": "^6.3.3",
		"react-use-measure": "^2.1.1",
		"simplebar-react": "^2.4.1",
		"use-immer": "^0.7.0",
		"zustand": "^3.6.9"
	}
}
