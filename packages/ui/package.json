{
	"name": "ui",
	"version": "0.0.0",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"sideEffects": false,
	"private": true,
	"license": "MIT",
	"files": [
		"dist/**"
	],
	"scripts": {
		"storybook": "start-storybook --no-open -s public -p 6006",
		"storybook:build": "build-storybook -s public",
		"test": "jest --clearCache --detectOpenHandles --forceExit --coverage",
		"build": "tsup \"src/**/*.{ts,tsx}\" --format esm,cjs --dts --external react",
		"dev": "tsup \"src/**/*.{ts,tsx}\" --format esm,cjs --watch --dts --external react",
		"check-types": "tsc --noemit",
		"eslint": "TIMING=1 eslint src --fix",
		"lint": "npm run eslint && npm run check-types",
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
	},
	"devDependencies": {
		"@babel/core": "^7.20.5",
		"@storybook/addon-actions": "^6.5.14",
		"@storybook/addon-essentials": "^6.5.14",
		"@storybook/addon-interactions": "^6.5.14",
		"@storybook/addon-links": "^6.5.14",
		"@storybook/addon-viewport": "^6.5.14",
		"@storybook/builder-vite": "^0.1.39",
		"@storybook/react": "^6.5.14",
		"@storybook/testing-library": "^0.0.13",
		"@vitejs/plugin-react": "^1.3.0",
		"autoprefixer": "^10.4.0",
		"css-loader": "5.2.6",
		"postcss-import": "^15.0.0",
		"postcss-nesting": "^10.2.0",
		"sass": "^1.49.9",
		"sass-loader": "10.1.1",
		"storybook-dark-mode": "^2.0.4",
		"style-loader": "2.0.0",
		"tsconfig": "*",
		"design": "*",
		"tsup": "^5.11.13",
		"typescript": "4.9.4",
		"vite": "^2.9.9"
	},
	"dependencies": {
		"@radix-ui/popper": "^0.1.0",
		"@radix-ui/react-accordion": "^1.0.0",
		"@radix-ui/react-alert-dialog": "^1.0.0",
		"@radix-ui/react-avatar": "^1.0.0",
		"@radix-ui/react-checkbox": "^1.0.0",
		"@radix-ui/react-dialog": "^1.0.0",
		"@radix-ui/react-dropdown-menu": "^0.1.6",
		"@radix-ui/react-hover-card": "^1.0.0",
		"@radix-ui/react-icons": "^1.0.3",
		"@radix-ui/react-popover": "^0.1.6",
		"@radix-ui/react-portal": "^1.0.0",
		"@radix-ui/react-select": "^1.0.0",
		"@radix-ui/react-slider": "^1.0.0",
		"@radix-ui/react-tabs": "^0.1.5",
		"@radix-ui/react-toggle": "^0.1.4",
		"@radix-ui/react-tooltip": "^0.1.7",
		"@stitches/react": "^1.2.8",
		"@storybook/react": "^6.4.15",
		"@types/react": "^17.0.39",
		"@types/react-dom": "^17.0.11",
		"framer-motion": "^6.5.1",
		"postcss-custom-media": "^9.0.1",
		"react-use-measure": "^2.1.1",
		"simplebar-react": "^2.4.1",
		"use-immer": "^0.7.0",
		"zustand": "^4.1.1"
	}
}
