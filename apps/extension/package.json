{
	"name": "z3us-extension",
	"version": "2.0.43",
	"type": "module",
	"scripts": {
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
		"dev": "DEV_TOOLS=true yarn start:radix",
		"start:radix": "RADIX=true yarn start",
		"start": "vite --mode development",
		"build:dev": "DEV_TOOLS=true yarn build:radix",
		"build:radix": "RADIX=true yarn build",
		"build": "npm_config_build_from_source=true tsc --build && vite build --mode production",
		"build:copy-as-firefox": "node src/helpers/firefox",
		"build:release": "yarn build:radix && yarn build:copy-as-firefox",
		"preview": "vite preview",
		"check-types": "tsc --noemit",
		"eslint": "eslint --fix --ignore-path .eslintignore --ext .js,.ts,.tsx .",
		"lint": "yarn eslint && yarn check-types",
		"test": "jest --maxWorkers=1  --detectOpenHandles --forceExit --passWithNoTests",
		"intl:extract": "formatjs extract 'src/**/*.{ts,tsx}' '../../packages/ui/src/**/*.{ts,tsx}' --ignore='**/*.d.ts' --out-file src/locales/en.json --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
		"intl:compile": "formatjs compile-folder --ast src/locales src/locales/compiled",
		"intl:clean": "node src/helpers/intl-clean"
	},
	"dependencies": {
		"@radix-ui/react-avatar": "^1.0.2",
