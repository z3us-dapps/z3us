{
	"extends": "tsconfig/nextjs.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@src/*": ["src/*"],
			"rxjs": ["node_modules/rxjs"],
			"rxjs/*": ["node_modules/rxjs/*"]
		}
	},
	"include": [
		"chrome.ts",
		"firefox.ts",
		"vite.config.ts",
		"src/**/*.d.ts",
		"src/**/*.ts",
		"src/**/*.tsx",
	],
}
